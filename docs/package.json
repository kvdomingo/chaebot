{
  "name": "chaebot",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.3.1",
    "bootstrap": "^4.6.2",
    "cloudinary-react": "^1.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactstrap": "^8.10.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "importOrder": [
      "^react$",
      "^react-dom",
      "^react(.*)",
      "<THIRD_PARTY_MODULES>",
      "^[./]"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true,
    "jsxSingleQuote": false,
    "printWidth": 120,
    "quoteProps": "consistent",
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "all"
  }
}
